{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-1",
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-2",
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-3",
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-1/*",
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-2/*",
                "arn:aws:s3:::salesforecast-ai-dev-sagemaker-bucket-3/*"
            ]
        },
        {
            "Sid": "AllowSageMakerCreateAppOperations",
            "Effect": "Allow",
            "Action": "sagemaker:*App",
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource": "*"
        }
    ]
}